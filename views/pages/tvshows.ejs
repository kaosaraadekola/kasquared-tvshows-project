<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Game of Throne Episodes</title>
</head>
<body>

    <ul>
        <% for (let i = 0; i < gameOfThronesEpisodes.length; i++) { %>
            <li><%= gameOfThronesEpisodes[i].name %> - <%= 'S' + gameOfThronesEpisodes[i].season.toString().padStart(2, '0') + 'E' + gameOfThronesEpisodes[i].number.toString().padStart(2, '0') %></li>
            <br>
            <img id="image" src="<%= gameOfThronesEpisodes[i].image.medium %>">
            <br>
            <%= gameOfThronesEpisodes[i].summary.split("<p>")[1].split ("</p>")[0] %>
            <hr/>
          <% } %>
          

    </ul>
    


    
</body>
</html> 